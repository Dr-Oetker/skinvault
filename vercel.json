{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install --omit=optional",
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.7"
    },
    "api/**/*.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "22.12.0"
    }
  }
}