{
  "buildCommand": "npm install --omit=optional && npm run build:safe",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install --omit=optional",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 